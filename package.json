{
  "name": "kadkilat-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "pages:build": "npm run lint && npm run type-check && npx @cloudflare/next-on-pages",
    "pages:build:ci": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev .vercel/output/static --port 8788",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "deploy:production": "npm run pages:build && wrangler pages deploy .vercel/output/static --env production",
    "cf:login": "wrangler auth login",
    "cf:whoami": "wrangler auth whoami"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-simple-maps": "3.0.0",
    "swiper": "^11.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.9.0",
    "@cloudflare/workers-types": "^4.20240222.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-simple-maps": "^3.0.4",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.9.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^32.3.0",
    "wrangler": "^3.30.0"
  }
}
